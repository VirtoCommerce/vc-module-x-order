<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <IsPackable>True</IsPackable>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="8.0.0" PrivateAssets="All" />
    <PackageReference Include="VirtoCommerce.OrdersModule.Core" Version="3.840.0-alpha.1310-vcst-2563" />
    <PackageReference Include="VirtoCommerce.Xapi.Core" Version="3.900.0" />
    <PackageReference Include="VirtoCommerce.XCart.Core" Version="3.901.0-alpha.71-vcst-2563" />
    <PackageReference Include="VirtoCommerce.XCatalog.Core" Version="3.902.0-alpha.63-vcst-2563" />
  </ItemGroup>
</Project>