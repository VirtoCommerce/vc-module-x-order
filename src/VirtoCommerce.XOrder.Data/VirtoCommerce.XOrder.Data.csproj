    <PackageReference Include="VirtoCommerce.XCatalog.Data" Version="3.919.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\VirtoCommerce.XOrder.Core\VirtoCommerce.XOrder.Core.csproj" />
  </ItemGroup>
</Project>
